{
    "compilerOptions": {
        "allowJs": false,
        "forceConsistentCasingInFileNames": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "../../server/cgi",
        "preserveConstEnums": true,
        "pretty": true,
		"removeComments": true,
        "sourceMap": true,
        "strict": true,
        "target": "esnext",
    },
    "include": [
        "./**/*"
    ]
}