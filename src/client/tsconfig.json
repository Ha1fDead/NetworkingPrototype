{
    "compilerOptions": {
        "allowJs": false,
        "forceConsistentCasingInFileNames": true,
        "module": "es2015",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "../../cgi/client/",
        "preserveConstEnums": true,
        "pretty": true,
        "removeComments": true,
        "sourceMap": true,
        "strict": true,
        "target": "esnext",
    },
    "include": [
        "./**/*"
    ]
}